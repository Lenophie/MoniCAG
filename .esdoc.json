{
  "source": "./src/FRONT-END/js",
  "destination": "./doc",
  "excludes": ["./src/FRONT-END/js/template-dashboard"],
  "plugins": [
    {
      "name": "esdoc-standard-plugin",
      "option": {
        "unexportedIdentifier": {"enable": true},
        "lint": {"enable": true},
        "coverage": {"enable": true},
        "brand": {
          "title": "MoniCAG",
          "description": "Monitor CAG Inventory !"
        },
        "manual": {
          "asset": "./src/DOC/assets",
          "files": [
            "./changelog.md",
            "./src/DOC/resources.md",
            "./src/DOC/UML-modelization.md"
          ]
        }
      }
    }
  ]
}